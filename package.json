{
  "name": "ai-education-assistant",
  "version": "1.0.0",
  "description": "ИИ-помощник для выпускников 9 класса в выборе дальнейшего пути",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start"
  },
  "keywords": ["education", "ai", "assistant", "career-guidance"],
  "author": "AI Education Assistant Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
